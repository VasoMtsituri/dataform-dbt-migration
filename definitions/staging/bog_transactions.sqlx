config {
    type: "view",
    assertions: {
        uniqueKey: ["rrn"],
        nonNull: ["rrn", "transaction_date", "transaction_amount"]
    }
}

select
    transaction_date,
    transaction_amount,
    transaction_ccy,
    commission,
    commission_ccy,
    settlement_amount,
    settlement_ccy,
    operation_type,
    rrn
from ${ref("bog")}